<templateSet group="user">
  <template name="//=" value="// ===================================&#10;// $END$&#10;// ===================================" description="block comment" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="//-" value="// -----------------------------------&#10;// $END$&#10;// -----------------------------------" description="line comment" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="--" value="&lt;!-- end $END$ --&gt;" description="close tag description" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
</templateSet>